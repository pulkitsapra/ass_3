#include "my_lib.h"



void main()
{
    int no, ch, e;
 
    printf("\n 1 - Push");
    printf("\n 2 - Pop");
    printf("\n 5 - Exit");
    printf("\n 6 - Dipslay");
    
 
  
 
    while (1)
    {
        printf("\n Enter choice : ");
        scanf("%d", &ch);
 
        switch (ch)
        {
        case 1:
            printf("Enter data : ");
            scanf("%d", &no);
            push(no);
            break;
        case 2:
            pop();
            break;
        
        case 5:
            exit(0);
        case 6:
            display();
            break;
        
        default :
            printf(" Wrong choice, Please enter correct choice  ");
            break;
        }
    }
}
